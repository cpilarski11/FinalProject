<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cwpila14.finalproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartScreenStuff.MainActivity"
            android:hardwareAccelerated="false">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BioStuff.BioActivity" />
        <activity android:name=".BioStuff.StarterPokemonActivity" />
        <activity android:name=".GameStuff.MainGameActivity" />
        <activity android:name=".TutorialStuff.TutorialActivity" />
        <activity android:name=".GameStuff.PokedexActivity" />
        <activity android:name=".GameStuff.BagActivity"></activity>
    </application>

</manifest>